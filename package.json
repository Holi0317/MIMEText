{
  "name": "mimetext",
  "version": "v3.0.21",
  "description": "RFC 2822 compliant raw email message generator written in node.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./dist/entrypoints/node.mjs",
      "require": "./dist/entrypoints/node.cjs"
    },
    "./browser": {
      "import": "./dist/entrypoints/browser.mjs",
      "require": "./dist/entrypoints/browser.cjs"
    },
    "./node": {
      "import": "./dist/entrypoints/node.mjs",
      "require": "./dist/entrypoints/node.cjs"
    },
    "./gas": {
      "import": "./dist/entrypoints/gas.mjs",
      "require": "./dist/entrypoints/gas.cjs"
    }
  },
  "types": "./dist/entrypoints/node.d.ts",
  "main": "./dist/entrypoints/node.cjs",
  "scripts": {
    "setup": "exit 0",
    "compile": "exit 0",
    "create-cts": "exit 0",
    "build": "unbuild",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "docs": "typedoc --entryPointStrategy expand --includeVersion --excludePrivate ./src/entrypoints/node.ts ./src/entrypoints/gas.ts ./src/entrypoints/browser.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/MIMEText.git"
  },
  "keywords": [
    "rfc-2822",
    "mime-message",
    "email-message"
  ],
  "author": "Murat GÃ¶zel <murat@gozel.com.tr>",
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/muratgozel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muratgozel/MIMEText/issues"
  },
  "homepage": "https://github.com/muratgozel/MIMEText#readme",
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.332.0",
    "@aws-sdk/client-sesv2": "^3.332.0",
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.3",
    "@types/google-apps-script": "^1.0.59",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "node-releaser": "^2.0.0-beta.4",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "js-base64": "^3.7.5",
    "mime-types": "^2.1.35"
  },
  "overrides": {
    "xml2js": "0.5.0"
  }
}
